FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/MMiflyMSC/iflyMSCSDK"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/MMiflyMSC"
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/MMiflyMSC/iflyMSCSDK/sunflower"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/MMiflyMSC"
OTHER_LDFLAGS = $(inherited) -ObjC -l"SunFlower" -l"c++" -l"z" -framework "AVFoundation" -framework "AddressBook" -framework "AudioToolbox" -framework "CoreTelephony" -framework "iflyMSC"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
